var sortColors = function(nums) {
    let left = 0; 
    let mid  = 0
    let right = nums.length-1;

    while(mid <=right){
       if(nums[mid] == 0){
         [nums[mid], nums[left]] = [nums[left], nums[mid]];
         mid++;
         left++;
       }else if(nums[mid] == 1){
        mid++;
       }else{
         [nums[mid], nums[right]]=[nums[right], nums[mid]];
          right--;
       }
    }
    return nums
};
