var coinChange = function(coins, amount) {
    if(amount < 1){
        return 0;
    }

    let dp = new Array(amount+1).fill(Infinity);
    dp[0] = 0;

    for(let i = 1; i<= amount; i++){
        for(let coin of coins){
            if(coin <= i && dp[i-coin] !== Infinity){
                dp[i] = Math.min(dp[i], 1 + dp[i-coin]);
            }
        }
    }
    if(dp[amount] === Infinity){
        return -1;
    }

    return dp[amount];
};
