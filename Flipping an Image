var flipAndInvertImage = function(image) {
     let n = image.length;
    let ans = [];
    for(let i=0; i<n; i++){
        let current = [];
        for(let j=0; j<n; j++){
            if(image[i][j] == 0){
                current.unshift(1);
            }else{
                current.unshift(0);
            }
        }
        ans.push(current);
    }
    return ans;
};
