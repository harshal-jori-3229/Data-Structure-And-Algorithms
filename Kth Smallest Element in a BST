var kthSmallest = function(root, k) {
    let count = 0;
    let result = -1;

    function inorder(node){
        if (!node || result !== -1) return;
        inorder(node.left);
        count++;
        if(count === k){
            result = node.val;
            return;
        }
        inorder(node.right);
    }
    inorder(root);
    return result;
};
