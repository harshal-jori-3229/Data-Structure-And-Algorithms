var singleNonDuplicate = function(nums) {
    let left = 0; 
    let right = nums.length;

    while(left < right ){
        let mid = Math.floor(left + (right-left)/2);

        if(mid % 2 == 0){
            mid--;
        }

        if(nums[mid] == nums[mid+1]){
            left = mid + 1;
        }else{
            right = mid;
        }
    }
    return nusm[left];
};
