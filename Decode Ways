var numDecodings = function(s) {
    let n = s.length;
    let dp = new Array(n + 1).fill(0);
    dp[0] = 1;
    dp[1] = s[0] == '0' ? 0 : 1;

    for(let i=2; i<= n; i++){
        let oneDigit = Number(s.substring(i - 1, i));
        let twoDigit = Number(s.substring(i - 2, i));

        if(oneDigit >= 1){
            dp[i] += dp[i-1];
        }
        if(twoDigit>=10 && twoDigit <= 26){
            dp[i] += dp[i-2];
        }
    }

    return dp[n]
};
